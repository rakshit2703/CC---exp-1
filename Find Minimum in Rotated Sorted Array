#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    int findMin(vector<int>& nums) {
        int low = 0, high = nums.size() - 1;

        while (low < high) {
            int mid = low + (high - low) / 2;

            if (nums[mid] > nums[high]) {
                // Minimum is in right part
                low = mid + 1;
            } else {
                // Minimum is in left part (including mid)
                high = mid;
            }
        }
        return nums[low];
    }
};
